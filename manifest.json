{
  "manifest_version": 3,
  "name": "Subito Scraper",
  "version": "1.0",
  "description": "Scrapes real estate data from Subito.it",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "*://*.subito.it/*"
  ]
}
